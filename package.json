{
  "name": "anota",
  "description": "SVG annotation",
  "scripts": {
    "lint": "eslint src",
    "static": "babel static --out-dir dist --copy-files",
    "compile": "babel src/main.js --out-file dist/anota.js --source-maps",
    "build": "npm run lint && npm run static && npm run compile",
    "dev": "npm run build && browser-sync start --server 'dist' --files 'dist'"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.4.0",
    "browser-sync": "^2.18.8",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-import": "^2.2.0"
  }
}
